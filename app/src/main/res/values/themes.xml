<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.Stale" parent="Theme.MaterialComponents.DayNight.NoActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">@color/blue</item>
        <item name="colorPrimaryVariant">@color/gray_dark</item>
        <item name="colorOnPrimary">@color/white</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/electric</item>
        <item name="colorSecondaryVariant">@color/electric</item>
        <item name="colorOnSecondary">@color/black</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor" tools:targetApi="l">?attr/colorPrimaryVariant</item>
        <!-- Customize your theme here. -->
    </style>

    <style name="OutlinedButton_1" parent="Widget.MaterialComponents.Button.OutlinedButton">
    </style>

    <style name="DarkProgressIndicator" parent="Widget.MaterialComponents.CircularProgressIndicator">
        <item name="indicatorColor">#3F9A3F</item>
        <item name="indicatorSize">56dp</item>
        <item name="trackThickness">3dp</item>
    </style>

    <style name="ShapeAppearanceOverlay.App.Circle" parent="">
        <item name="cornerSize">50%</item>
    </style>

</resources>